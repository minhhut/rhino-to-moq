<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>0.2.0</Version>
    <Authors>Minh Nguyen</Authors>
    <Description>Extensions to quickly migrate old Rhino Mocks to Moq</Description>
    <PackageProjectUrl>https://www.nuget.org/packages/RhinoMocksToMoq/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/minhhut/rhino-to-moq</RepositoryUrl>
    <PackageTags>rhino moq adapter</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Moq" Version="4.13.1" />
  </ItemGroup>

</Project>
